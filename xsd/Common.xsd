					<xsd:documentation>
						<definition>The external Requisition code</definition>
						<datatype>string</datatype>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="monsterId" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="InterestsType">
