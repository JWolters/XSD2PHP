	<xsd:complexType name="WeekendEnumStr">
		<xsd:annotation>
			<xsd:documentation>
0	No
1	Yes
2	I prefer weekends
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="monsterId" type="WeekendIdEnum" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
