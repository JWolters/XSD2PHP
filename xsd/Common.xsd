		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="RequestDocElement" type="xsd:string"/>
				<xsd:element name="Status" type="StatusType"/>
				<xsd:element name="Response" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:annotation>
								<xsd:documentation>Any response ok, lax means
								look for xsd but not an error if not found.</xsd:documentation>
							</xsd:annotation>
							<xsd:any namespace="##any" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
