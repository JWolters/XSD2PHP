	<xsd:complexType name="AwardType">
		<xsd:sequence>
			<xsd:element name="AwardName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>TheAward name</definition>
						<datatype>string</datatype>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AwardDescription" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>The Awards Description</definition>
						<datatype>string</datatype>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="monsterId" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="IndustriesType">
		<xsd:sequence>
			<xsd:element name="Industry" type="IndustryType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IndustryType">
		<xsd:sequence>
			<xsd:element name="IndustryName" type="MonsterIndustriesEnumStr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>An ID used to describe an Industry type.</definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CoverLetterType">
		<xsd:sequence>
			<xsd:element name="LetterTitle" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition> For applicants : The title of the letter the user includes in the application - available inbound only; not a field that is exported. For JobSeekers : The title of the letter. It is exported outbound.</definition>
						<datatype>Text</datatype>
						<datalength>50</datalength>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LetterBody" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>For applicants : The body of the letter the user includes in the application. For JobSeekers : The body of the letter</definition>
						<datatype>Text</datatype>
						<datalength>6000</datalength>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="letterId" type="xsd:string"/>
		<xsd:attribute name="action" use="optional">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="add"/>
					<xsd:enumeration value="addOrReplace"/>
					<xsd:enumeration value="update"/>
					<xsd:enumeration value="updateOrClone"/>
					<xsd:enumeration value="delete"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="LettersType">
		<xsd:sequence>
			<xsd:element name="Letter" type="CoverLetterType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SearchInformationType">
		<xsd:annotation>
			<xsd:documentation>
				This object contains data about a search that was made. It includes recruiter data for the person performing the search, data describing the search, and the date the search occured.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RecruiterReference" type="RecruiterReferenceType"/>
			<xsd:element name="Keywords" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						All keywordsdata that qualified the search. This field has a maximum size of 4000 characters.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Channel">
				<xsd:annotation>
					<xsd:documentation>
						The channel used to qualify the search. No channel value indicates channel data was not used to qualify the search.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SearchDate" type="DateTimeType">
				<xsd:annotation>
					<xsd:documentation>
						This field indicates the date and time the search occured.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CreatedDate" type="DateTimeType">
				<xsd:annotation>
					<xsd:documentation>
						This field indicates the date and time the search was recorded in the DB.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BoardName" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QueryBehavior" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						Defines how keywords are used in a search. This field has a maximum size of 225 characters.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecentJobTitle" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						The desired recent job title requested in the search if any. This field has a maximum size of 70 characters.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TargetJobTitle" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						The desired target job title requested in the search if any. This field has a maximum size of 70 characters.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecentOrTargetJobTitle" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						The desired recent or target job title requested in the search if any. This field has a maximum size of 70 characters.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecentEmployeerCompanyName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						The desired recent employeer copmany name requested in the search if any.  This field has a maximum size of 70 characters.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecentSchoolName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						The desired recent school name requested in the search if any. This field has a maximum size of 25 characters.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecentJobDescription" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						The desired recent job description. 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TextResumeId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
            The desired recent textual resume identifier requested in the search if any.
          </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="JobStatus" type="JobStatusEnumStr" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						Each job status indicates a requested job status in the search if any.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="JobType" type="UserPreferredJobCodeEnumStr" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						Each job type indicates a requested job status in the search if any.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EducationLevel" type="EducationDegreeEnumStr" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						The list of education levels requested during the search. No education levels indicate that no education level criteria was used to qualify the search.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CareerLevel" type="CareerLevelEnumStr" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						The list of career levels requested during the search. No career levels indicate that no career level criteria was used to qualify the search.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="JobCategory" type="JobCategoryType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						The list of company job categories requested during the search. No company job categories  indicate that no company job category criteria was used to qualify the search.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ResidenceLocation" type="LocationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						The list of residence locations requested during the search. No residence locations indicate that no residence location criteria was used to qualify the search.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ResidencePostalCodeRadius" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						The postal code radius requested during the search. No values indicate that no residence postal code radius criteria was used to qualify the search.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TargetWorkLocation" type="LocationType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						The list of target work locations requested during the search. No target work locations indicate that no target work location criteria was used to qualify the search.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WillingToRelocate" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						A flag indicating the desired value requested in the search for ones willingness to relocate. no value indicates that this data was not used to qualify the search.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WorkAuthorizationCountries" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						A list of countries that of which the candidate should be authorized to work in one. No countries indicate that country criteria was not used to qualify the search.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TargetSalaryData" type="JobSalaryType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						Target salary data desired in the search. No salary data indicates that salary criteria was not used to qualify the search.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurrentSecurityClearance" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						A flag indicatingwhether security clearance was required during this search. No value indicates that security clearance was not used to qualify the search.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ModifiedDateMinimumAge" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						Used to qualify how recently a resume was edited for it to be returned.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ModifiedDateMaximumAge" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						Used to qualify how recently a resume was edited for it to be returned.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MaximumResultSet" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						The maximum number of resumes to return.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MonsterOccupationalClassification" type="OccupationalClassificationTier2Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						The Monster Occupational Classification (MOC) desired in the search. No MOC indicates that MOC criteria was not used to qualify the search.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!--
			<xsd:element name="TargetWorkLocationBoolean" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
						No Idea
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TargetWorkSite" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						No Idea
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TargetWorkIndustry" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						No Idea
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			-->
		</xsd:sequence>
		<xsd:attribute name="monsterId" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
						Monster's internal unique identifier for the search.
					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="SearchesInformationType">
		<xsd:sequence>
			<xsd:element name="SearchInformation" type="SearchInformationType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="NameAndAbbreviationType">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Abbreviation" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="monsterId" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
						Monster's internal unique identifier for the item.
					</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="JobSalaryType">
		<xsd:sequence>
			<xsd:element name="Currency" type="CurrencyType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The currency of the salary</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SalaryMin" type="MoneyType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The minimum salary</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SalaryMax" type="MoneyType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The maximum salary</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CompensationType" type="CompensationTypeEnumStr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The frequency of pay</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SalaryDescription" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Description of the total compensation package provided by the company</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SalaryRange" type="SalaryRangeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used for chief jobs only.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SalaryRangeType">
		<xsd:annotation>
			<xsd:documentation>For Chief only. A lookup into
			a table of text salary ranges.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SalaryMin" type="xsd:string" minOccurs="0"/>
			<xsd:element name="SalaryMax" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="salaryRangeId" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="DocumentReferenceType">
		<xsd:sequence>
			<xsd:element name="DocumentType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>DocumentType of the requested Document (DD-214, Transcript etc.,)</definition>
						<datatype>Text</datatype>
						<datalength>50</datalength>
						<validation>Required</validation>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="dd-214"/>
						<xsd:enumeration value="of-306"/>
						<xsd:enumeration value="sf-50"/>
						<xsd:enumeration value="sf-15"/>
						<xsd:enumeration value="transcript"/>
						<xsd:enumeration value="other"/>
						<xsd:enumeration value="cover_letter"/>
						<xsd:enumeration value="secure_resume"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="FileType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>FileType of the requested Document (.doc, .pdf, .jpg etc.,)</definition>
						<datatype>Text</datatype>
						<datalength>10</datalength>
						<validation>Required</validation>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="doc"/>
						<xsd:enumeration value="pdf"/>
						<xsd:enumeration value="jpg"/>
						<xsd:enumeration value="jpeg"/>
						<xsd:enumeration value="docx"/>
						<xsd:enumeration value="gif"/>
						<xsd:enumeration value="rtf"/>
						<xsd:enumeration value="png"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DocumentName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>DocumentName of the requested Document sent for all document types</definition>
						<datatype>Text</datatype>
						<datalength>100</datalength>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="documentID" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>
					<definition>Unique Identifier of the requested Document.  It is a string ID</definition>
					<datatype>Text</datatype>
					<datalength>100</datalength>
					<validation>Required</validation>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DocumentContentType">
		<xsd:complexContent>
			<xsd:extension base="DocumentReferenceType">
				<xsd:sequence>
					<xsd:element name="DocumentContent" type="xsd:base64Binary" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
								<definition>Encoded Binary Content of the requested Document</definition>
								<datatype>base64Binary</datatype>
								<validation>Required</validation>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:simpleType name="WorkingHoursPerWeekType">
		<xsd:annotation>
			<xsd:documentation>
				<definition>This provides a value resticted to a max of 5 digits with a max of 2 digits after decimal point</definition>
				<datatype>decimal</datatype>
				<validation>Required</validation>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:maxInclusive value="168"/>
			<xsd:minInclusive value="0.00"/>
			<xsd:fractionDigits value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ResumeIconsType">
		<xsd:sequence>
			<xsd:element name="ResumeIcon" type="ResumeIconType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ResumeIconType">
		<xsd:sequence>
			<xsd:element name="ResumeIconName" type="ResumeIconsEnumString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>The Resume Icon type</definition>
						<datatype>Int</datatype>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="UserDriversLicensesType">
		<xsd:sequence>
			<xsd:element name="UserDriversLicense" type="UserDriversLicenseType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="UserDriversLicenseType">
		<xsd:sequence>
			<xsd:element name="UserDriversLicenseName" type="UserDriversLicenseEnumStr" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>The driver's license associated with the user</definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IssueDate" type="DateTimeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>The date associated with the driver's license</definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DisabilityType">
		<xsd:sequence>
			<xsd:element name="DisabilityDescription" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>A description of the disability</definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="status" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="true"/>
					<xsd:enumeration value="false"/>
					<xsd:enumeration value="declinedToIdentify"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="VictimOfTerrorismType">
		<xsd:attribute name="status" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="true"/>
					<xsd:enumeration value="false"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="ExConvictType">
		<xsd:attribute name="status" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="true"/>
					<xsd:enumeration value="false"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:simpleType name="RecruiterActionType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="add"/>
			<xsd:enumeration value="addOrUpdate"/>
			<xsd:enumeration value="update"/>
			<xsd:enumeration value="ignore"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="Document" type="DocumentType"/>
	<xsd:complexType name="DocumentType">
		<xsd:annotation>
			<xsd:documentation>
				<definition>Defines a document</definition>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="File" type="xsd:base64Binary" minOccurs="0"/>
			<xsd:element name="FileName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<definition>Name of file, including file extension.</definition>
						<datatype>Text</datatype>
						<datalength>100</datalength>
						<validation>Required</validation>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FileContentType" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<definition>Content/mime type associated with file.</definition>
						<datatype>Text</datatype>
						<validation>Required</validation>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="documentAction" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="add"/>
					<xsd:enumeration value="deleteAndAdd"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="PropertiesType">
		<xsd:sequence>
			<xsd:element name="Property" type="PropertyType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<definition>Holder for all extension data related to a monster business object</definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PropertyType">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						<definition>Name of property extension</definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Value" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						<definition>Value of property extension</definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OidClaimType">
		<xsd:sequence>
			<xsd:element name="OidProviderId" type="LookupOIDProvidersEnumStr">
				<xsd:annotation>
					<xsd:documentation>
						<definition>this is a provider id for the claim</definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ClaimIdentifier" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>this is the OID Guid</definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProviderGuid" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>this is the Provider OID Guid. Used for seamless login</definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="CompanyName">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="255"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="ChannelID" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>
        ChannelID
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="UserID" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>
        UserID of the User of Job to return.
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="JobID" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>
				<definition>JobID of a job.</definition>
				<datatype>String</datatype>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>
