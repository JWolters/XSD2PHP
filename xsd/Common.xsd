			<xsd:element name="Interest" type="InterestType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="InterestType">
		<xsd:sequence>
			<xsd:element name="InterestName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>The interest name</definition>
						<datatype>string</datatype>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="monsterId" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="IndustryCertificationsType">
