			<xsd:element name="IndustryCertification" type="IndustryCertificationType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IndustryCertificationType">
		<xsd:sequence>
			<xsd:element name="CertificationName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>The name of the certification </definition>
						<datatype>string</datatype>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VendorName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>The name of the vendor that provided the certification </definition>
						<datatype>string</datatype>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MonthAcquired" type="MonthType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>The Month the certification was aquired [1-12]</definition>
						<datatype>int</datatype>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="YearAcquired" type="YearType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>The Year  the certification was aquired </definition>
						<datatype>Int</datatype>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Summary" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<definition>The summary of the certification </definition>
						<datatype>string</datatype>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="monsterId" type="xsd:string"/>
	</xsd:complexType>
	<xsd:complexType name="AwardsType">
		<xsd:sequence>
			<xsd:element name="Award" type="AwardType" minOccurs="0" maxOccurs="unbounded"/>
